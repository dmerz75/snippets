from cycler import cycler
print matplotlib.rcParams['axes.prop_cycle']
mycolors = ['k', 'r', 'g', 'b','c','m','lime','darkorange','sandybrown','hotpink']
ax1.set_prop_cycle(cycler('color',mycolors))
# reset:
ax1.set_prop_cycle(cycler('color',mycolors)
